{
  "name": "ordersapp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint ."
  },
  "dependencies": {
    "@apollo/react-hooks": "^3.1.3",
    "apollo-boost": "^0.4.4",
    "apollo-cache-inmemory": "^1.6.3",
    "apollo-client": "^2.6.4",
    "apollo-link": "^1.2.13",
    "apollo-link-error": "^1.1.12",
    "apollo-link-http": "^1.5.16",
    "axios": "^0.19.0",
    "babel-loader": "^8.0.6",
    "babel-plugin-syntax-trailing-function-commas": "^6.22.0",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-react-native": "^4.0.1",
    "clean-webpack-plugin": "^3.0.0",
    "fs": "^0.0.1-security",
    "graphql": "^14.5.8",
    "init": "^0.1.2",
    "json-loader": "^0.5.7",
    "react": "16.9.0",
    "react-native": "0.61.2",
    "react-native-gesture-handler": "^1.4.1",
    "react-native-reanimated": "^2.10.0",
    "react-native-webpack": "^0.1.1",
    "react-navigation": "^4.0.10",
    "react-navigation-stack": "^1.10.2",
    "webpack": "^4.41.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/runtime": "^7.6.3",
    "@jonny/react-native-mock": "^0.4.2",
    "@react-native-community/eslint-config": "^0.0.5",
    "@storybook/addon-actions": "^5.2.5",
    "@storybook/addon-links": "^5.2.5",
    "@storybook/addons": "^5.2.5",
    "@storybook/react-native": "^5.2.5",
    "@storybook/react-native-server": "^5.2.5",
    "babel-jest": "^24.9.0",
    "detox": "^14.5.1",
    "enzyme": "^3.10.0",
    "enzyme-adapter-react-16": "^1.15.1",
    "eslint": "^6.5.1",
    "eslint-plugin-detox": "^1.0.0",
    "jest": "^24.9.0",
    "metro-react-native-babel-preset": "^0.56.0",
    "prop-types": "^15.7.2",
    "react-docgen": "^4.1.1",
    "react-dom": "^16.11.0",
    "react-native-testing-library": "^1.11.1",
    "react-test-renderer": "16.9.0",
    "webpack-dev-server": "^3.9.0"
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./tests/setup.js"
    ]
  },
  "detox": {
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/ordersApp.app",
        "build": "xcodebuild -workspace ios/POSAppPoC.xcworkspace -scheme ordersApp -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "device": {
          "type": "iPhone X"
        }
      },
      "android.emu.debug": {
        "binaryPath": "android/app/build/outputs/apk/debug/app-debug.apk",
        "build": "cd android && ./gradlew assembleDebug assembleAndroidTest -DtestBuildType=debug && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Nexus_10_API_28"
        }
      },
      "android.emu.release": {
        "binaryPath": "android/app/build/outputs/apk/release/app-release.apk",
        "build": "cd android && ./gradlew assembleRelease assembleAndroidTest -DtestBuildType=release && cd ..",
        "type": "android.emulator",
        "device": {
          "avdName": "Nexus_10_API_28"
        }
      }
    },
    "runner-config": "e2e/config.json",
    "test-runner": "jest"
  }
}
